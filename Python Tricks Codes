from itertools import product
list1=range(1,3)
list2=range(4,6)
list3=range(7,9)

for item1 in list1:
    for item2 in list2:
        for item3 in list3:
            print (item1+item2+item3)
            
print ("new code")            
for i1,i2,i3 in product(list1,list2,list3):
    
    print (i1+i2+i3)


